extend type QueryResolver{
    accounts:[Account]
    account(id:ID!):Account
    getEmployeesByNumberLike(number:String!):[Account]
}
extend type MutationResolver{
    newAccount(account:AccountInput!):Account
}
input AccountInput{
    number:String!
    employeeId:Int
}
type Account{
    id:ID!
    number:String
    employee:Employee
}

